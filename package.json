		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@vitest/coverage-istanbul": "^0.29.2",
		"humanize-duration": "^3.28.0",
		"jsdom": "^21.1.0",
		"node-plop": "^0.31.1",
		"prettier": "^2.8.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"ts-node": "^10.9.1",
